deseq2_dea(dataType = "gene", Name = "HIF3A", env = UCS_legacy_gene_tumor_data)
prepar_path_enrich(File = "Results.Completed.DESeq2", env = UCS_legacy_gene_tumor_data)
GOnto(condition = "Upregulated", Tool = "DESeq2", env = UCS_legacy_gene_tumor_data)
GOnto(condition = "Downregulated", Tool = "DESeq2", env = UCS_legacy_gene_tumor_data)
GOnto(condition = "ALL", Tool = "DESeq2", env = UCS_legacy_gene_tumor_data)
GSEA(Tool = "DESeq2", env = UCS_legacy_gene_tumor_data)
DO.REAC.ENRICH(Tool = "DESeq2", env = UCS_legacy_gene_tumor_data)

CrossThem(FinalData = "EBSeq", n.pack = 3,
          packageNames = c("DESeq2", "edgeR", "EBSeq"), env = UCS_legacy_gene_tumor_data)
prepar_path_enrich(File = "Results.Completed.crossed", env = UCS_legacy_gene_tumor_data)
GOnto(condition = "Upregulated", Tool = "CrossTable.EBSeq", env = UCS_legacy_gene_tumor_data)
GOnto(condition = "Downregulated", Tool = "CrossTable.EBSeq", env = UCS_legacy_gene_tumor_data)
GOnto(condition = "ALL", Tool = "CrossTable.EBSeq", env = UCS_legacy_gene_tumor_data)
GSEA(Tool = "CrossTable.EBSeq", env = UCS_legacy_gene_tumor_data)
DO.REAC.ENRICH(Tool = "CrossTable.EBSeq", env = UCS_legacy_gene_tumor_data)

