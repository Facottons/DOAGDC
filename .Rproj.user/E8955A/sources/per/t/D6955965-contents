# "uc002peg.3 - HIF3a2"
concatenate_files(dataType = "isoform", normalization = TRUE, Name = "uc002peg.3",
          tumorData = TRUE, dataBase = "legacy",
          workDir = "/media/virtual_drive/desktop/", tumor = "TGCT")
groups_identification(dataType = "isoform", group.number = 2, Name = "uc002peg.3",
         modelName = "E", reRunPlots = FALSE, env = UCS_legacy_gene_tumor_data,
         tumor = "UCS", dataBase = "legacy", workDir = "/media/virtual_drive/desktop/GDCRtools")
groups_identification(dataType = "isoform", group.number = 2, Name = "uc002peg.3",
         modelName = "E", reRunPlots = TRUE, env = UCS_legacy_gene_tumor_data,
         tumor = "UCS", dataBase = "legacy", workDir = "/media/virtual_drive/desktop/GDCRtools",
         uncertaintyCutoff = 0.05)
concatenate_files(dataType = "gene", normalization = FALSE,
          tumorData = TRUE, dataBase = "legacy",
          workDir = "/media/virtual_drive/desktop/", tumor = "TGCT")

edger_dea(dataType = "gene", Name = "HIF3A", Method = "exactTest", workDir = "/media/virtual_drive/desktop", env = UCS_legacy_gene_tumor_data)
prepar_annotations(Name = "HIF3A", workDir = "/media/virtual_drive/desktop", env = UCS_legacy_gene_tumor_data) #there's some default parameters
prepar_path_enrich(File = "Results.Completed.edgeR", env = UCS_legacy_gene_tumor_data,Tool = "edgeR")
GOnto(condition = "Upregulated", Tool = "edgeR", env = UCS_legacy_gene_tumor_data)
GOnto(condition = "Downregulated", Tool = "edgeR", env = UCS_legacy_gene_tumor_data)
GOnto(condition = "ALL", Tool = "edgeR", env = UCS_legacy_gene_tumor_data)
GSEA(Tool = "edgeR", env = UCS_legacy_gene_tumor_data)
DO.REAC.ENRICH(Tool = "edgeR", env = UCS_legacy_gene_tumor_data)

